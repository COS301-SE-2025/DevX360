# DevX360 API Environment Variables
# Copy this file to .env and fill in your values

# Database
MONGODB_URI=mongodb+srv://username:password@cluster.mongodb.net/devx360?retryWrites=true&w=majority

# JWT Secret (generate a strong random string)
JWT_SECRET=your-super-secret-jwt-key-here

# OpenAI API Key
OPENAI_API_KEY=sk-your-openai-api-key-here

# GitHub Tokens (for API access)
GITHUB_TOKEN_1=ghp_your-github-token-1-here
GITHUB_TOKEN_2=ghp_your-github-token-2-here

# MCP API Token
MCP_API_TOKEN=your-mcp-api-token-here

# Server Configuration
PORT=5000
NODE_ENV=development